<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.projectmodule"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="16" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.projectmodule.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.projectmodule.AddActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.example.projectmodule.RemoveActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.example.projectmodule.LogActivity"
            android:label="@string/app_name" >
        </activity>
        
        <activity
            android:name="com.example.projectmodule.AdminActivity"
            android:label="@string/app_name" >
        </activity>
        
        <activity
            android:name="com.example.projectmodule.EditLogActivity"
            android:label="@string/app_name" >
        </activity>
        
        <activity
            android:name="com.example.projectmodule.WebActivity"
            android:label="@string/app_name" >
        </activity>
        
    </application>

    <!--  Internet Permissions -->
    <uses-permission android:name="android.permission.INTERNET" />
    
</manifest>